<div class="col-md-12">
  {{loading-slider isLoading=loading  color=hexColorsArray}}
  <div class="c-notification__container">
    {{#each notifications as |notification|}}
    {{notification-message notification=notification}}
    {{/each}}
  </div>
  <div class="center text-center">
    {{#link-to  'application' }}

    <h3>Order App Demo</h3>
    {{/link-to}}
    {{#paper-toolbar}}
    <div class="md-toolbar-tools">
      {{#link-to  'dashboard.orders'}}
      {{#paper-button}}
      Orders
      {{/paper-button}}
      {{/link-to}}

      {{#link-to  'dashboard.orders.order'}}
      {{#paper-button}}
      Order
      {{/paper-button}}
      {{/link-to}}
    </div>
    {{/paper-toolbar}}

  </div>
  <div class="col-md-12">
    {{#paper-card}}
    {{#paper-card-content}}
    {{outlet}}
    {{/paper-card-content}}
    {{/paper-card}}
  </div>
</div>
