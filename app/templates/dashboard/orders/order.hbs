

{{#paper-card}}
{{#paper-card-content}}


  {{paper-input label="Order Title" required=true value=model.title icon="description"  required-errortext="Order title is required."}}

<table class="table table-bordered">
  <th>No.</th><th>Name of Item </th><th>Quantity</th><th>Rate</th><th>Amount</th>
  {{#each model.items as |item index|}}
  <tr>
    <td>{{index}}</td><td>{{item.name}}</td><td>{{item.quantity}}</td><td>{{item.rate}}</td><td>{{item.amount}}</td>
      </tr>
  {{/each}}
</table>

{{/paper-card-content}}
{{/paper-card}}
